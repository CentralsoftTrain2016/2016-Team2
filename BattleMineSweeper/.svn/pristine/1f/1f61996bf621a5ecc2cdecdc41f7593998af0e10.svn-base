package service.bean;

import java.util.ArrayList;
import java.util.List;

public class MassStateBean {

	List<MassUpdateBean> massList = new ArrayList<MassUpdateBean>();
	boolean isMyTurn;
	boolean isFinished;
	QuizBean quizBean;
	ItemBean itemBean;
	PlayerBean myPlayerBean;
	PlayerBean enemyPlayerBean;

	public PlayerBean getMyPlayerBean() {
		return this.myPlayerBean;
	}

	public void setMyPlayerBean(PlayerBean myPlayerBean) {
		this.myPlayerBean = myPlayerBean;
	}

	public PlayerBean getEnemyPlayerBean() {
		return this.enemyPlayerBean;
	}

	public void setEnemyPlayerBean(PlayerBean enemyPlayerBean) {
		this.enemyPlayerBean = enemyPlayerBean;
	}

	public QuizBean getQuizBean() {
		return quizBean;
	}

	public void setQuizBean(QuizBean quizBean) {
		this.quizBean = quizBean;
	}

	public ItemBean getItemBean() {
		return itemBean;
	}

	public void setItemBean(ItemBean itemBean) {
		this.itemBean = itemBean;
	}

	public List<MassUpdateBean> getMassList() {
		return massList;
	}

	public void getMassList(List<MassUpdateBean> massList_p) {
		this.massList = massList_p;
	}


	public void addMassUpdateBean(MassUpdateBean massUpdateBean_p) {
		this.massList.add(massUpdateBean_p);
	}

	public boolean getIsMyTurn() {
		return isMyTurn;
	}
	public void setIsMyTurn(boolean isMyTurn_p) {
		this.isMyTurn = isMyTurn_p;
	}

	public boolean getIsFinished() {
		return isFinished;
	}
	public void setIsFinished(boolean isFinished) {
		this.isFinished = isFinished;
	}

}
