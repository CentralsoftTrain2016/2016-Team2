package service.bean;

import domain.value.CHARACTERID;
import domain.value.DIFFICULTYID;

public class ConfigBean extends Bean{

	/**
	 * コンストラクタ
	 */
	public ConfigBean() {
		super();
	}

	/**
	 * 難易度IDのゲッター
	 *
	 * @return 難易度ID
	 */
	public DIFFICULTYID getDifficultyID() {
		return difficultyID;
	}

	/**
	 * キャラクターIDのゲッター
	 *
	 * @return キャラクターID
	 */
	public CHARACTERID getCharacterID() {
		return characterID;
	}

	/**
	 * 難易度IDのセッター
	 *
	 * @param difficultyID_p 難易度ID
	 */
	public void setDifficultyID(DIFFICULTYID difficultyID_p) {
		this.difficultyID = difficultyID_p;
	}

	/**
	 * キャラクターIDのセッター
	 *
	 * @param characterID_p キャラクターID
	 */
	public void setCharacterID(CHARACTERID characterID_p) {
		this.characterID = characterID_p;
	}

	private DIFFICULTYID difficultyID = null;
	private CHARACTERID characterID = null;
}
